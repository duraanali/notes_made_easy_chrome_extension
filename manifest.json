{
    "name": "Notes Made Easy",
    "version": "1.0",
    "description": "Have you ever wanted to take a quick note, this is your extension",
    "permissions": ["storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "manifest_version": 2,
    "page_action": {
        "default_popup": "popup.html"
      }
  }